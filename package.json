{
  "name": "css-uwindsor-bot",
  "version": "2.0.0",
  "description": "Bot for UWindsor CSS discord",
  "main": "index.ts",
  "scripts": {
    "dev": "ts-node-dev --respawn --exit-child -r tsconfig-paths/register src/index.ts",
    "start": "ts-node -r tsconfig-paths/register src/index.ts"
  },
  "author": "CSS Software Devs css@uwindsor.ca",
  "license": "MIT",
  "repository": "git@github.com:UWindsorCSS/uwindsor-discord-bot.git",
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": false,
    "quoteProps": "as-needed",
    "trailingComma": "es5",
    "bracketSpacing": false,
    "arrowParens": "always",
    "endOfLine": "lf"
  },
  "eslintConfig": {
    "node": true,
    "es6": true
  },
  "dependencies": {
    "@prisma/client": "^5.1.1",
    "discord-api-types": "^0.37.52",
    "discord.js": "^14.12.1",
    "dotenv": "^16.3.1",
    "fuse.js": "^6.5.3",

    "js-yaml": "^4.1.0",
    "mathjax-node": "^2.1.1",
    "pino": "^8.15.0",
    "pino-pretty": "^10.2.0",
    "pino-roll": "^1.0.0-rc.1",
    "svg2img": "^1.0.0-beta.2"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.5",
    "@types/mathjax": "^0.0.37",
    "@types/node": "^20.4.9",
    "prisma": "^5.1.1",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
  }
}
